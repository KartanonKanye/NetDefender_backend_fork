{
  "name": "backend_netdefender",
  "version": "0.0.1",
  "description": "Backend for NetDefender web app for ELEC-C7420 Basic Principles in Networking. Developed by Kabir Bissessar and Maria Lappalainen.",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "default": "dist/index.js"
      }
    }
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsc",
    "prestart": "rm -rf dist && npm run build",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}"
  },
  "author": "Kabir Bissessar",
  "license": "MIT",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.4.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "chalk": "^5.3.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "express": "^4.18.2",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "onchange": "^7.1.0",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
